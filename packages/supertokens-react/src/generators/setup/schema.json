{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "Setup",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name of the app that you want to setup?"
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the project (used for linting)",
      "alias": "t"
    },
    "directory": {
      "type": "string",
      "description": "A directory where the project is placed",
      "alias": "d"
    },
    "STAppName": {
      "type": "string"
    },
    "STApiDomain": {
      "type": "string"
    },
    "STWebDomain": {
      "type": "string"
    },
    "STApiBasePath": {
      "type": "string"
    },
    "STWebBasePath": {
      "type": "string"
    }
  },
  "required": ["name"]
}
